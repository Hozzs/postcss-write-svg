{
  "name": "gh-pages",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "npm run build:codepen && npm run build:index",
    "build:codepen": "browserify src/codepen.js | babel --presets=es2015 | uglifyjs --compress --mangle > codepen.js",
    "build:index": "browserify src/index.js | babel --presets=es2015 | uglifyjs --compress --mangle > index.js"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "browserify": "^13.1.1",
    "dacoda": "^2.0.0",
    "postcss-scss": "^0.4.0",
    "postcss-write-svg": "^3.0.1",
    "uglify-js": "^2.7.5"
  }
}
